{
  "name": "@tractorbeamai/reusables",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tractorbeamai/reusables.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "turbo run build",
    "changeset": "changeset",
    "clean": "turbo run clean && rm -rf node_modules",
    "dev": "turbo run dev",
    "format": "prettier --write .",
    "lint": "turbo run lint:eslint lint:types lint:prettier",
    "lint:eslint": "turbo run lint:eslint",
    "lint:prettier": "prettier --check .",
    "lint:types": "turbo run lint:types",
    "release": "turbo run build && changeset publish"
  },
  "prettier": "@tractorbeam/prettier-config",
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@tractorbeam/prettier-config": "workspace:*",
    "eslint": "^9.39.2",
    "jiti": "^2.6.1",
    "prettier": "^3.7.4",
    "turbo": "^2.6.3"
  },
  "packageManager": "pnpm@10.26.0",
  "engines": {
    "node": ">=22"
  }
}
