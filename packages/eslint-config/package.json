{
  "name": "@tractorbeamai/eslint-config",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/configs/index.ts",
      "types": "./src/configs/index.ts"
    },
    "./utils": {
      "import": "./src/utils.ts",
      "types": "./src/utils.ts"
    },
    "./auto": {
      "import": "./src/configs/auto.ts",
      "types": "./src/configs/auto.ts"
    },
    "./*": {
      "import": "./src/configs/*.ts",
      "types": "./src/configs/*.ts"
    }
  },
  "peerDependencies": {
    "eslint": "^9"
  },
  "dependencies": {
    "@eslint/json": "^0.13.2",
    "@nx/eslint-plugin": "21.4.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^54.1.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-unicorn": "^60.0.0",
    "eslint-plugin-zod": "^1.4.0",
    "globals": "^16.3.0",
    "tslib": "^2.3.0",
    "typescript-eslint": "^8.41.0"
  },
  "devDependencies": {
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "jiti": "^2.5.1"
  }
}
