{
  "name": "@tractorbeam/prettier-config",
  "version": "0.1.1",
  "description": "Shared Prettier configuration for Tractorbeam projects",
  "keywords": [
    "tractorbeam",
    "tractorbeamai",
    "prettier",
    "prettierconfig",
    "prettier-config",
    "tailwind",
    "sort-imports"
  ],
  "homepage": "https://github.com/tractorbeamai/reusables/tree/main/packages/prettier-config#readme",
  "bugs": {
    "url": "https://github.com/tractorbeamai/reusables/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tractorbeamai/reusables.git",
    "directory": "packages/prettier-config"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/**",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist *.tsbuildinfo",
    "postinstall": "pnpm run build",
    "lint:types": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.6.14"
  },
  "devDependencies": {
    "@tractorbeam/typescript-config": "workspace:*",
    "prettier": "^3.5.3",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "prettier": ">=3.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
